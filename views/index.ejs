<%- include('header'); -%>
  <section id="content">
    <div class="container vidcontent">
      <h2> Self-Driving-Car Training and Implementation Video </h2>
      <div class="col-md-6 col-sm-12"><iframe width="420" height="315" src="https://youtube.com/embed/SzU-jXJ3z64"></iframe></div>
      <div class="col-md-6 col-sm-12"><iframe width="420" height="315" src="https://youtube.com/embed/3SXc2VyggY4"></iframe></div>
    </div>
    <div class="container">
      <h1> Predictions from Raspberry Pi 4 </h1>
      <div class="row explanation">
        <div class="col-md-4 col-sm-6"><h3>Steering > 0 Adalah prediksi ke kanan </h3></div>
        <div class="col-md-4 col-sm-6"><h3>Steering < 0 Adalah prediksi ke kiri</h3></div>
        <div class="col-md-4 col-sm-12"><h3>Steering = 0 Adalah prediksi maju </h3></div>
      </div>
          <% for (var i=0; i<nameArray.length; i++) { %>
            <div class="row">
            <% 	if(typeof nameArray[i] !== "undefined") {%>
              <% var ind1 = nameArray[i].indexOf('_'); %>
              <% var ind2 = nameArray[i].indexOf('_', ind1 + 1); %>
              <% var substring = nameArray[i].substring(ind2); %>
              <div class="card col-md-4">
                <img class="card-img-top" src="/img/<%=nameArray[i]%>">
                <p>Steering: <%= substring.slice(1) %></p>
              </div> <% }; %>
            <% 	if(typeof nameArray[i+1] !== "undefined") {%>
              <% ind1 = nameArray[i+1].indexOf('_'); %>
              <% ind2 = nameArray[i+1].indexOf('_', ind1 + 1); %>
              <% substring = nameArray[i+1].substring(ind2); %>
              <div class="card col-md-4">
                <img class="card-img-top" src="img/<%=nameArray[i+1]%>">
                <p>Steering: <%= substring.slice(1) %></p>
              </div> <% }; %>
            <% 	if(typeof nameArray[i+2] !== "undefined") {%>
              <% ind1 = nameArray[i+2].indexOf('_'); %>
              <% ind2 = nameArray[i+2].indexOf('_', ind1 + 1); %>
              <% substring = nameArray[i+2].substring(ind2); %>
              <div class="card col-md-4">
                <img class="card-img-top" src="img/<%=nameArray[i+2]%>">
                <p>Steering: <%= substring.slice(1) %></p>
              </div> <% }; %>
              <% i = i + 2 ;%>
            </div>
            <% }; %>
    </div>
  </section>


<%- include('footer'); -%>
